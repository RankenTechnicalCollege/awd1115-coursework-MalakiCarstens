@model IEnumerable<Hot4.Models.Customer>

<h2>Customers</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model)
        {
            <tr>
                <td>@c.Username</td>
                <td>@c.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create" class="btn btn-primary">Add Customer</a>


